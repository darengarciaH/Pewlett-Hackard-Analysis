Departments
-
dept_no varchar pk FK >- Dept_Emp.dept_no
dept_name varchar

Employees
-
emp_no pk FK >- Titles.emp_no
birth_date date
first_name varchar
last_name varchar
gender varchar
hire_date date

Managers
-
dept_no varchar fk - Departments.dept_no
emp_no int pk fk - Employees.emp_no
from_date
to_date

Salaries
-
emp_no FK >- Employees.emp_no
salary
from_date
to_date

Titles
-
emp_no FK >- Salaries.emp_no
title
from_date
to_date

Dept_Emp
-
dept_no
emp_no
from_date
to_date

